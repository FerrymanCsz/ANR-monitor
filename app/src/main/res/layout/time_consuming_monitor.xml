<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="200dp"
    android:id="@+id/background_color"
    android:layout_marginTop="20dp"
    android:layout_marginBottom="20dp"
    android:layout_marginLeft="2dp"
    android:background="@color/card1">




    <TextView
        android:id="@+id/collection_type"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="collection_type"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188">

    </TextView>

    <TextView
        android:id="@+id/wall_time"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:text="wall_time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/collection_type" />

    <TextView
        android:id="@+id/cup_time"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:text="cup_time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wall_time" />

    <TextView
        android:id="@+id/counts"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:text="counts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cup_time" />

    <TextView
        android:id="@+id/msg_id"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="msg ID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/counts"/>


</androidx.constraintlayout.widget.ConstraintLayout>